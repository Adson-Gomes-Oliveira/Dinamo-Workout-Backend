{
  "name": "dinamo-api",
  "version": "1.0.0",
  "description": "An API to get data from a workout database, getting infos such as workout registers, progression, health tracking and more! ",
  "main": "index.js",
  "scripts": {
    "view": "npm ls --depth 0",
    "test": "mocha ./tests/**/ --exit",
    "debug": "nodemon ./src/server.ts",
    "start": "ts-node src/server.ts",
    "coverage": "nyc --all npm run test",
    "resetall": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
  "author": "Adson Gomes Oliveira",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.13",
    "chai": "^4.3.6",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
